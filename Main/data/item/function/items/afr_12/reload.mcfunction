execute if score @s gun.reload_time matches 50 run playsound block.iron_trapdoor.open player @a ~ ~ ~ 1 2

execute if score @s gun.reload_time matches 40 run playsound block.piston.extend player @a ~ ~ ~ 1 2

execute if score @s gun.reload_time matches 20 run playsound block.piston.contract player @a ~ ~ ~ 1 2

execute if score @s gun.reload_time matches 10 run playsound block.iron_trapdoor.close player @a ~ ~ ~ 1 2